{
    "name": "kerrialn/ci-gen",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-simplexml": "*",
        "ext-json": "*",
        "symfony/config": "^5.0",
        "symfony/console": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/finder": "^5.0",
        "symfony/http-kernel": "^5.0",
        "symfony/yaml": "^5.0",
        "symplify/package-builder": "^7.2",
        "symplify/autowire-array-parameter": "^7.2",
        "nette/utils": "^3.0",
        "phar-io/version": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "phpstan/phpstan": "^0.12.3",
        "symplify/easy-coding-standard": "^7.2",
        "symplify/phpstan-extensions": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "CIConfigGen\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CIConfigGen\\Tests\\": "tests"
        }
    },
    "scripts": {
        "check-cs": "vendor/bin/ecs check src tests --ansi",
        "fix-cs": "vendor/bin/ecs check src tests --ansi --fix",
        "phpstan": "vendor/bin/phpstan analyse --ansi --error-format symplify"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
